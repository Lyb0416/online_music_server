<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyb.olinemusicserver.mapper.DownloadRecordMapper">

    <resultMap id="BaseResultMap" type="com.lyb.olinemusicserver.model.domain.DownloadRecord">
            <id property="id" column="id" />
            <result property="userId" column="user_id" />
            <result property="songId" column="song_id" />
            <result property="createTime" column="create_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,song_id,create_time
    </sql>
        <!-- 查询所有下载记录 -->
    <select id="selectAllRecords" resultType="com.lyb.olinemusicserver.model.domain.DownloadRecord">
        select * from download_record
    </select>
</mapper>
