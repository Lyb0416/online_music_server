<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lyb.olinemusicserver.mapper.SongListMapper">

    <resultMap id="BaseResultMap" type="com.lyb.olinemusicserver.model.domain.SongList">
            <id property="id" column="id" />
            <result property="title" column="title" />
            <result property="pic" column="pic" />
            <result property="introduction" column="introduction" />
            <result property="style" column="style" />
    </resultMap>

    <sql id="Base_Column_List">
        id,title,pic,introduction,style
    </sql>
    <delete id="deleteIds">
        delete FROM song_list WHERE id IN (${ids}) ;
    </delete>
</mapper>
